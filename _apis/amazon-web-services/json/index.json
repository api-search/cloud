---
layout: none
---           
{% for providers in site.apis %}
{% if providers.name == 'Amazon Web Services' %}
{
"name": "{{ providers.name }}",
"description": "{{ providers.description | strip_newlines | strip_html }}",
"image": "{{ providers.image }}",
"url": "{{ providers.url }}",
"created": "{{ providers.created }}",
"modified": "{{ providers.modified }}",
"specificationVersion": "{{ providers.specificationVersion }}",
"tags": {{ providers.tags | jsonify }},
"apis": [
{% for api in providers.apis %}
{
"name": "{{ api.name | strip_newlines }}",
"slug": "{{ providers.name | slugify }}",
"description": "{{ api.description | strip_newlines | strip_html }}",
"image": "{{ api.image }}",
"humanURL": "{{ api.humanURL }}",
"baseURL": "{{ api.baseURL }}",
"tags": {{ api.tags | jsonify }},
"properties": {{ api.properties | jsonify }},
"common": {{ api.common | jsonify }}
}{% unless forloop.last %},{% endunless %}
{% endfor %}
]
"maintainers": {{ providers.maintainers | jsonify }},
"overlays": {{ providers.overlays | jsonify }},
"aid": "{{ api.aid }}"
{% endif %}
{% endfor %}
